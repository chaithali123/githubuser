    "\n",
    "class Power:\n",
    "    def __init__ (self,x,n):\n",
    "        self.x=x\n",
    "        self.n=n\n",
    "    def powerr(self):\n",
    "        powerr= self.x ** self.n\n",
    "        return powerr\n",
    "p=Power(10,2)\n",
    "p.powerr()"
