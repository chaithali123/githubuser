    "for i in range(0, n):#iterating\n",
    "    ele = int(input())\n",
    "   \n",
    "\n",
    "    lst.append(ele) #add elements to list\n",
    "    sum=sum+ele\n",
    "    ele+=1\n",
    "    \n",
    " \n",
    "\n",
    "     \n",
    "print(sum)"
